				<munit-tools:then-return >
					<munit-tools:variables >
						<munit-tools:variable key="vEducationData" value='#[[{AQB__ContactExternalID__c: "000000002",Id:"ajhgklafsdgherg34"}]]' mediaType="application/java" />
					</munit-tools:variables>
				</munit-tools:then-return>
